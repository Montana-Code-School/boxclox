{
  "name": "boxclox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf static",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && node ./bin/www",
    "start": "node ./bin/www",
    "lint": "eslint client config models routes app.js || true",
    "postinstall": "webpack --config ./webpack.config.prod.js --progress --colors",
    "deploy": "npm run build && modulus deploy --project-name stripe-demo"
  },
  "dependencies": {
    "babel": "^6.1.18",
    "babel-runtime": "5.6.18",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "react": "^0.14.3",
    "react-countdown-clock": "^1.0.3",
    "react-dom": "^0.14.3",
    "react-native": "^0.15.0",
    "react-redux": "^4.0.0",
    "reactify": "^1.1.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "5.6.18",
    "babel-eslint": "^4.1.6",
    "babel-loader": "5.3.1",
    "html-webpack-plugin": "1.6.0",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  }
}
