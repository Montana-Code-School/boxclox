{
  "name": "boxclox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf static",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && nodemon ./bin/www",
    "start": "node ./bin/www",
    "lint": "eslint client config models routes app.js || true",
    "postinstall": "webpack --config ./webpack.config.prod.js --progress --colors",
    "deploy": "npm run build && modulus deploy --project-name boxclox",
    "test": "npm run lint"
  },
  "dependencies": {
    "addons": "^0.1.2",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^1.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "history": "^1.13.1",
    "jquery": "^2.1.4",
    "linked": "^0.2.0",
    "material-ui": "^0.13.4",
    "mixin": "^0.2.0",
    "morgan": "~1.6.1",
    "react": "^0.14.3",
    "react-addons-linked-state-mixin": "^0.14.3",
    "react-blink": "^1.0.3",
    "react-dom": "^0.14.3",
    "react-localstorage": "^0.2.8",
    "react-redux": "^3.0.0",
    "react-tap-event-plugin": "^0.2.1",
    "reactify": "^1.1.1",
    "serve-favicon": "~2.3.0",
    "state": "^0.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.14",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.0.2",
    "babel-preset-es2015": "^6.0.8",
    "babel-preset-react": "^6.0.2",
    "chokidar": "^1.2.0",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^3.10.0",
    "html-webpack-plugin": "1.6.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  }
